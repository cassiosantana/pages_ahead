<div id="<%= dom_id supplier %>">
  <p>
    <strong>Name:</strong>
    <%= supplier.name %>
  </p>

  <p>
    <strong>Cnpj:</strong>
    <%= supplier.cnpj %>
  </p>

  <p>
    <strong>Account:</strong>
    <span id="test-account_with_digit_value"><%= supplier.account_with_digit %></span>
  </p>

  <div>
    <p><strong>Parts:</strong></p>
    <ul id="test-parts_list">
      <% supplier.parts.each do |part| %>
        <li><%= part.name %></li>
      <% end %>
    </ul>
  </div>
</div>